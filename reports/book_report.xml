<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_library_book">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <div class="page">
                    <h2>Book Report: <span t-field="doc.name"/></h2>
                    <p>Author: <span t-field="doc.author"/></p>
                    <p>Status: <span t-field="doc.status"/></p>
                    <p>Rating: <span t-field="doc.rating"/></p>
                    <p>Added Date: <span t-field="doc.added_date"/></p>
                    <p>Notes: <span t-field="doc.notes"/></p>
                </div>
            </t>
        </t>
    </template>
</odoo>